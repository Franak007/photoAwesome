<div>
    {% set medias = get_last_five_media() %}
    {% dump(medias) %}
    {% if app.user %}
        {# je suis connecté #}
        <a href="{{ path('app_logout') }}">Se déconnecter</a>

        {% if is_granted('ROLE_ADMIN') %}
            <a href="{{ path('app_admin') }}">Administration</a>
        {% endif %}

    {% else %}
        {# Je ne suis pas connecté #}
        <a href="{{ path('app_login') }}">Se connecter</a>
        <a href="{{ path('app_register') }}">S'enregistrer</a>
    {% endif %}
<br>
    {% for media in medias %}
    {{ media.title}} /
    {% endfor %}
</div>